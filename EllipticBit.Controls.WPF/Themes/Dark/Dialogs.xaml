			xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			xmlns:sys="clr-namespace:System;assembly=mscorlib">

	<!-- Viewer -->
	<SolidColorBrush x:Key="ViewerBackground" Color="#66EEEEEE" />

	<!-- Dialog -->
